{
  "private": true,
  "workspaces": [
    "example",
    "packages/*"
  ],
  "scripts": {
    "generate": "yarn --cwd example generate",
    "migrate": "yarn --cwd example migrate",
    "test": "yarn --cwd example test",
    "event:live": "yarn --cwd example emit:live",
    "serve:live": "yarn --cwd example serve:live",
    "setup:live": "yarn --cwd example setup:live",
    "populate:live": "yarn --cwd example populate:live",
    "purge:live": "yarn --cwd example purge:live",
    "push:live": "yarn --cwd example push:live",
    "query:live": "yarn --cwd example query:live",
    "event:test": "yarn --cwd example emit:test",
    "setup:test": "yarn --cwd example setup:test",
    "populate:test": "yarn --cwd example populate:test",
    "purge:test": "yarn --cwd example purge:test",
    "push:test": "yarn --cwd example push:test",
    "query:test": "yarn --cwd example query:test",
    "event:dev": "yarn --cwd example emit:dev",
    "serve:dev": "yarn --cwd example serve:dev",
    "setup:dev": "yarn --cwd example setup:dev",
    "populate:dev": "yarn --cwd example populate:dev",
    "purge:dev": "yarn --cwd example purge:dev",
    "push:dev": "yarn --cwd example push:dev",
    "query:dev": "yarn --cwd example query:dev",
    "build": "yarn build:incept && yarn build:generators && yarn build:plugins",
    "build:incept": "yarn --cwd packages/incept build",
    "build:generators": "yarn build:types && yarn build:inquire && yarn build:ink",
    "build:types": "yarn --cwd packages/incept-types build",
    "build:ink": "yarn --cwd packages/incept-ink build",
    "build:inquire": "yarn --cwd packages/incept-inquire build",
    "build:plugins": "yarn build:i18n && yarn build:user && yarn build:api && yarn build:admin && yarn build:email",
    "build:i18n": "yarn --cwd packages/incept-i18n build",
    "build:admin": "yarn --cwd packages/incept-admin build",
    "build:user": "yarn --cwd packages/incept-user build",
    "build:api": "yarn --cwd packages/incept-api build",
    "build:email": "yarn --cwd packages/incept-email build",
    "build:example": "yarn --cwd example build"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5"
  }
}