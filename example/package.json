{
  "name": "incept-example",
  "version": "1.0.0",
  "private": true,
  "plugins": [
    "./plugins/app/plugin",
    "./plugins/store/plugin",
    "@stackpress/incept",
    "@stackpress/incept-types",
    "@stackpress/incept-inquire",
    "@stackpress/incept-ink",
    "@stackpress/incept-admin",
    "@stackpress/incept-i18n",
    "@stackpress/incept-user",
    "@stackpress/incept-api",
    "@stackpress/incept-email"
  ],
  "scripts": {
    "build": "yarn build:ts && yarn generate && yarn migrate && yarn build:app",
    "build:ts": "tsc",
    "build:app": "dotenv -e .env -- ts-node scripts/build.ts",
    "develop": "dotenv -e .env -- ts-node scripts/serve.ts",
    "emit": "dotenv -e .env -- ts-node scripts/event.ts",
    "generate": "dotenv -e .env -- ts-node scripts/generate.ts",
    "migrate": "dotenv -e .env -- ts-node scripts/migrate.ts",
    "populate": "dotenv -e .env -- ts-node scripts/populate.ts",
    "purge": "dotenv -e .env -- ts-node scripts/purge.ts",
    "push": "dotenv -e .env -- ts-node scripts/push.ts",
    "query": "dotenv -e .env -- ts-node scripts/query.ts",
    "start": "dotenv -e .env -- yarn --cwd build node build/scripts/serve.js",
    "test": "dotenv -e .env -- nyc ts-mocha tests/*.test.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@stackpress/incept": "0.1.25",
    "@stackpress/incept-admin": "0.1.25",
    "@stackpress/incept-api": "0.1.25",
    "@stackpress/incept-email": "0.1.25",
    "@stackpress/incept-i18n": "0.1.25",
    "@stackpress/incept-ink": "0.1.25",
    "@stackpress/incept-inquire": "0.1.25",
    "@stackpress/incept-types": "0.1.25",
    "@stackpress/incept-user": "0.1.25",
    "@stackpress/ink": "0.3.36",
    "@stackpress/ink-ui": "0.3.36",
    "@stackpress/ingest": "0.3.36",
    "@stackpress/inquire": "0.3.36",
    "@stackpress/inquire-pg": "0.3.36",
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.2.13",
    "@stackpress/ink-css": "0.3.36",
    "@stackpress/ink-dev": "0.3.36",
    "@stackpress/inquire-pglite": "0.3.36",
    "@types/chai": "4.3.20",
    "@types/js-cookie": "3.0.6",
    "@types/jsonwebtoken": "9.0.7",
    "@types/mocha": "10.0.10",
    "@types/pg": "8.11.10",
    "@types/node": "22.9.3",
    "chai": "4.5.0",
    "dotenv-cli": "7.4.4",
    "mocha": "10.8.2",
    "nyc": "17.1.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
