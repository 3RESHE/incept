{
  "name": "incept-example",
  "version": "1.0.0",
  "private": true,
  "plugins": [
    "./src/plugin",
    "@stackpress/incept",
    "@stackpress/incept-types",
    "@stackpress/incept-inquire",
    "@stackpress/incept-ink",
    "@stackpress/incept-admin",
    "@stackpress/incept-i18n",
    "@stackpress/incept-user",
    "@stackpress/incept-api",
    "@stackpress/incept-email"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/modules/util/event.ts",
    "generate": "dotenv -e .env.local -- ts-node src/modules/util/event.ts transform",
    "live": "NODE_ENV=production dotenv -e .env.local -- yarn incept",
    "migrate": "dotenv -e .env.local -- ts-node src/modules/util/migrate.ts",
    "serve": "dotenv -e .env.local -- ts-node src/modules/util/serve.ts",
    "test": "NODE_ENV=integration nyc ts-mocha tests/*.test.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@stackpress/incept": "0.1.15",
    "@stackpress/incept-admin": "0.1.15",
    "@stackpress/incept-api": "0.1.15",
    "@stackpress/incept-email": "0.1.15",
    "@stackpress/incept-i18n": "0.1.15",
    "@stackpress/incept-ink": "0.1.15",
    "@stackpress/incept-inquire": "0.1.15",
    "@stackpress/incept-types": "0.1.15",
    "@stackpress/incept-user": "0.1.15",
    "@stackpress/ink": "0.3.27",
    "@stackpress/ink-ui": "0.3.27",
    "@stackpress/ingest": "0.3.27",
    "@stackpress/inquire": "0.3.27",
    "@stackpress/inquire-pg": "0.3.27",
    "cookie": "1.0.2",
    "ethers": "6.13.4",
    "js-cookie": "3.0.5",
    "jsonwebtoken": "9.0.2",
    "mime": "4.0.4",
    "mustache": "4.2.0",
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.2.13",
    "@stackpress/ink-css": "0.3.27",
    "@stackpress/ink-dev": "0.3.27",
    "@stackpress/inquire-pglite": "0.3.27",
    "@types/chai": "4.3.20",
    "@types/js-cookie": "3.0.6",
    "@types/jsonwebtoken": "9.0.7",
    "@types/mocha": "10.0.10",
    "@types/pg": "8.11.10",
    "@types/node": "22.9.3",
    "chai": "4.5.0",
    "dotenv-cli": "7.4.4",
    "mocha": "10.8.2",
    "nyc": "17.1.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
