{
  "name": "incept-example",
  "version": "1.0.0",
  "private": true,
  "plugins": [
    "./src/plugin",
    "@stackpress/incept",
    "@stackpress/incept-types",
    "@stackpress/incept-inquire",
    "@stackpress/incept-ink",
    "@stackpress/incept-admin",
    "@stackpress/incept-i18n",
    "@stackpress/incept-user",
    "@stackpress/incept-api",
    "@stackpress/incept-email"
  ],
  "scripts": {
    "build": "tsc",
    "generate": "dotenv -e .env.local -- ts-node src/scripts/generate.ts",
    "migrate": "dotenv -e .env.local -- ts-node src/scripts/migrate.ts",
    "test": "NODE_ENV=integration nyc ts-mocha tests/*.test.ts",
    "serve:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/serve.ts",
    "emit:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/emit.ts",
    "populate:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/populate.ts",
    "purge:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/purge.ts",
    "push:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/push.ts",
    "query:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/query.ts",
    "setup:live": "NODE_ENV=production dotenv -e .env.local -- ts-node src/scripts/install.ts",
    "serve:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/serve.ts",
    "emit:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/emit.ts",
    "populate:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/populate.ts",
    "purge:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/purge.ts",
    "push:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/push.ts",
    "query:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/query.ts",
    "setup:test": "NODE_ENV=integration dotenv -e .env.local -- ts-node src/scripts/install.ts",
    "serve:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/serve.ts",
    "emit:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/emit.ts",
    "populate:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/populate.ts",
    "purge:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/purge.ts",
    "push:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/push.ts",
    "query:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/query.ts",
    "setup:dev": "NODE_ENV=development dotenv -e .env.local -- ts-node src/scripts/install.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@stackpress/incept": "0.1.0",
    "@stackpress/ink": "0.3.21",
    "@stackpress/ink-ui": "0.3.21",
    "@stackpress/ingest": "0.3.21",
    "@stackpress/inquire": "0.3.21",
    "@stackpress/inquire-pg": "0.3.21",
    "cookie": "1.0.2",
    "ethers": "6.13.4",
    "js-cookie": "3.0.5",
    "jsonwebtoken": "9.0.2",
    "mime": "4.0.4",
    "mustache": "4.2.0",
    "pg": "8.13.1"
  },
  "devDependencies": {
    "@electric-sql/pglite": "0.2.13",
    "@stackpress/ink-css": "0.3.21",
    "@stackpress/ink-dev": "0.3.21",
    "@stackpress/inquire-pglite": "0.3.21",
    "@types/chai": "4.3.20",
    "@types/js-cookie": "3.0.6",
    "@types/jsonwebtoken": "9.0.7",
    "@types/mocha": "10.0.10",
    "@types/pg": "8.11.10",
    "@types/node": "22.9.3",
    "chai": "4.5.0",
    "dotenv-cli": "7.4.4",
    "mocha": "10.8.2",
    "nyc": "17.1.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}
