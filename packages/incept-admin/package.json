{
  "name": "@stackpress/incept-admin",
  "version": "0.1.0",
  "license": "MIT",
  "description": "This package manages the theme of the admin in an Incept app.",
  "author": "Chris <chris@stackpress.io>",
  "homepage": "https://github.com/stackpress/incept",
  "bugs": "https://github.com/stackpress/incept/issues",
  "repository": "stackpress/incept",
  "plugins": [
    "./dist/plugin"
  ],
  "scripts": {
    "build": "yarn build:ts && yarn build:ink",
    "build:ts": "tsc",
    "build:ink": "copyfiles -u 1 ./src/**/*.ink ./src/**/**/*.ink ./dist"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "@stackpress/idea-transformer": "0.3.21",
    "@stackpress/incept": "0.1.0",
    "@stackpress/ingest": "0.3.21",
    "@stackpress/ink": "0.3.21",
    "@stackpress/ink-ui": "0.3.21",
    "papaparse": "5.5.1",
    "ts-morph": "24.0.0"
  },
  "devDependencies": {
    "@stackpress/ink-css": "0.3.21",
    "@stackpress/ink-dev": "0.3.21",
    "@types/papaparse": "5.3.15",
    "@types/node": "22.9.3",
    "copyfiles": "2.4.1",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}