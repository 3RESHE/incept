    "incept.d.ts",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "devDependencies": {
    "@types/node": "22.7.5",
    "copyfiles": "2.4.1",
    "rimraf": "6.0.1",
    "ts-morph": "23.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  }
}