{
  "name": "@stackpress/incept",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Idea transformer to Typescript types",
  "author": "Chris <chris@stackpress.io>",
  "homepage": "https://github.com/stackpress/incept",
  "bugs": "https://github.com/stackpress/incept/issues",
  "repository": "stackpress/incept",
  "bin": {
    "incept": "./bin.js"
  },
  "scripts": {
    "build": "yarn build:ts && yarn build:mv:components && yarn build:cp:components",
    "build:ts": "tsc",
    "build:mv:components": "rimraf ./components && mv ./dist/components .",
    "build:cp:components": "copyfiles -u 2 ./src/components/**/*.ink ./components"
  },
  "main": "index.js",
  "files": [
    "components",
    "dist",
    "index.js",
    "index.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "@stackpress/idea-parser": "0.1.9",
    "@stackpress/idea-transformer": "0.1.9",
    "@stackpress/ingest": "0.1.3",
    "@stackpress/ink": "0.1.10",
    "@stackpress/ink-css": "0.1.10",
    "@stackpress/ink-ui": "0.1.10",
    "@types/node": "22.7.5",
    "copyfiles": "2.4.1",
    "rimraf": "6.0.1",
    "ts-morph": "23.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  }
}