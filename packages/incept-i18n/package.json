    "index.d.ts",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "@stackpress/incept": "0.2.14"
  },
  "devDependencies": {
    "@types/node": "22.9.3",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
}